        SOIT_PO_NUM, SOSI_SCHDAT, SOSI_SHP_REF, SOMS_CUSCOD, SOMS_DPTCOD, SOMS_CURCOD,   
        DP_SHP_PLN.F_GET_SHPNM_NO(SOSI_DIVCOD,SOSI_RUNNUM,SOSI_SHISEQ,SOSI_ITMSEQ) SHPNM_NO, 
        DP_SO.F_GET_ITMNUM(SOSI_DIVCOD,SOIT_ITMRUNNUM) IPMS_ITMNUM  
 FROM   TSO_MST, TSO_ITM, TSO_SHI       
 WHERE  SOMS_DIVCOD = SOIT_DIVCOD       
 AND    SOMS_RUNNUM = SOIT_RUNNUM       
 AND    SOIT_DIVCOD = SOSI_DIVCOD       
 AND    SOIT_RUNNUM = SOSI_RUNNUM       
 AND    SOIT_ITMSEQ = SOSI_ITMSEQ       
 AND    SOMS_STSCOD = 'CO'        
)       
WHERE  SOSI_SCHQTY - TOT_SHIPPED_QTY > 0]]></SQLQuery>
