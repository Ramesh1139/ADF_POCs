  <SQLQuery><![CDATA[SELECT *         
FROM         
  (SELECT SOMS_CUSCOD, 
	SOMS_DPTCOD,      
	SOMS_DOCNBR SO_NO,    
	SOSI_SHISEQ,        	 
	SOIT_PO_NUM,   
	SOMS_CURCOD,        
	IPMS_ITMNUM,         
	SOSI_SCHDAT,         
    SOSI_DIVCOD,         
    SOSI_RUNNUM,         
    SOSI_ITMSEQ,         
    SOIT_ITMRUNNUM,         
    SOIT_CUSITM,         
    SOIT_SHPCOD,         
    SOMS_SRCOFFCOD,         
    SOSI_SCHQTY,         
    DP_SI.F_GET_TOT_SHIPPED_QTY(SOSI_DIVCOD, SOSI_RUNNUM, SOSI_SHISEQ, SOSI_ITMSEQ) TOT_SHIPPED_QTY,               
    SOSI_SHP_REF,         
    SOIT_ITMPRC,          
    SOIT_PBMS_RUNNUM,              
    SOMS_BRDCOD,         
    SOMS_SPNCOD,         
    SOMS_PAYTRMCOD,         
    SOMS_PAYTRMDES,         
    SOMS_BILDAT_REF,         
    DP_UTL.F_GET_TYPDESC('CPMS_BILDAT_REF',SOMS_BILDAT_REF) SOMS_BILDAT_REF_DES,    
    DP_SHP_PLN.F_GET_SHPNM_NO(SOSI_DIVCOD,SOSI_RUNNUM,SOSI_SHISEQ,SOSI_ITMSEQ) SHPNM_NO,  
    SOIT_WH_COD  
  FROM TSO_MST, TSO_ITM, TSO_SHI, TIP_MST         
  WHERE SOMS_DIVCOD = SOIT_DIVCOD         
  AND SOMS_RUNNUM   = SOIT_RUNNUM         
  AND SOIT_DIVCOD   = SOSI_DIVCOD         
  AND SOIT_RUNNUM   = SOSI_RUNNUM         
  AND SOIT_ITMSEQ   = SOSI_ITMSEQ         
  AND SOIT_DIVCOD   = IPMS_DIVCOD         
  AND SOIT_ITMRUNNUM= IPMS_RUNNUM         
  AND SOMS_STSCOD   = 'CO'         
  AND SOMS_DIVCOD   = :divcodVar         
  AND SOMS_RUNNUM   = NVL(:somsRunnumVar, SOMS_RUNNUM))         
WHERE SOSI_SCHQTY - TOT_SHIPPED_QTY > 0]]></SQLQuery>
