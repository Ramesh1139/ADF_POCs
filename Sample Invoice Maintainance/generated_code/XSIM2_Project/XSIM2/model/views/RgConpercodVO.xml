>
  <Variable
    Name="BV_SIMS_CUSCOD"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BV_G_DIVCOD"
    Kind="where"
    Type="java.lang.String"/>
    <DesignTime>
        <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
        <Attr Name="_isCodegen" Value="true"/>
        <Attr Name="_isExpertMode" Value="true"/>
    </DesignTime>
  <SQLQuery><![CDATA[select cpcp_surnam||' '||cpcp_fstnam conpernam, cpcp_dptcod, mpcd_des, cpcp_conpercod, cpcp_cusid, cpcp_buyid  
from tcp_conper, tmp_cusdpt
where cpcp_divcod = :BV_G_DIVCOD
and cpcp_cuscod = :BV_SIMS_CUSCOD
and cpcp_divcod = mpcd_divcod
and cpcp_dptcod = mpcd_dptcod
and (cpcp_clsdat is null or cpcp_clsdat > sysdate)
]]></SQLQuery>
