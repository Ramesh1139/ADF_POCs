      <Attr Name="_DisplaySize" Value="7"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.nordstrom.rpm.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
