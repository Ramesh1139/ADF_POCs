  <af:resource type="javascript">
    function selectRowCheckBox(event) {
        var nativeKeyPressEvent = event.getNativeEvent();
        var key = nativeKeyPressEvent.keyCode;
        var char = nativeKeyPressEvent.charCode;
        var control = nativeKeyPressEvent.ctrlKey;
        if (control &amp;&amp; key === 10) {            
            var table = AdfPage.PAGE.findComponentByAbsoluteId('r1:0:pc1:t1');            
            var rowKeySet = table.getSelectedRowKeys();
            for (rowKey in rowKeySet) {
                var selection = table.findComponent("sbc1", rowKey);
                if (selection.getValue().toString() == 'true')
                selection.setValue(false);
                else 
                selection.setValue(true);
            }
        }
    }

    function selectRowT2CheckBox(event) {
        var nativeKeyPressEvent = event.getNativeEvent();
        var key = nativeKeyPressEvent.keyCode;
        var char = nativeKeyPressEvent.charCode;
        var control = nativeKeyPressEvent.ctrlKey;
        if (control &amp;&amp; key === 10) {            
            var table = AdfPage.PAGE.findComponentByAbsoluteId('r1:0:pc2:t2');
            var rowKeySet = table.getSelectedRowKeys();
            for (rowKey in rowKeySet) {
                var selection = table.findComponent("sbc2", rowKey);
                if (selection.getValue().toString() == 'true')
                selection.setValue(false);
                else 
                selection.setValue(true);
            }
        }
    }
    
    function blockCharacters(event){            
        var _keyCode = event.getKeyCode(); 
        var _filterField = event.getCurrentTarget();       
        if (!((_keyCode &lt; 58) || (_keyCode > 95 &amp;&amp; _keyCode &lt; 106))) { 
            _filterField.setValue(null); 
            event.cancel(); 
        }
        if(_keyCode == 16){
            _filterField.setValue(null); 
            event.cancel();
        }
    } 
  </af:resource>
